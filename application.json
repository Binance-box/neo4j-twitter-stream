{
  "id": "/neo4j-twitter-stream",
  "env": {
    "NEO4J_URL": "bolt+routing://neo4j:dcos@core-neo4j.marathon.containerip.dcos.thisdcos.directory:7687",
    "TWITTER_KEYS": "consumerKey:consumerSecret:authToken:authSecret",
    "TWITTER_TERMS": "happy,sad"
